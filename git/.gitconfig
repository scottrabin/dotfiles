[core]
	whitespace = space-before-tab,trailing-space,cr-at-eol
	editor = code --wait
[alias]
	up = "!git remote update -p; git merge --ff-only @{u}"
	st = "!git status -sb"
[advice]
	statusHints = false
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	whitespace = red reverse
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[rerere]
	enabled = 1
[branch]
	autosetuprebase = always
[push]
	default = upstream
[difftool]
	prompt = false
[mergetool]
	prompt = false
[color]
	ui = true
