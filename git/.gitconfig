[core]
	whitespace=fix,indent-with-non-tab,trailing-space,cr-at-eol
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	whitespace = red reverse
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[rerere]
	enabled = 1
[branch]
	autosetuprebase = never
[push]
	default = upstream
[difftool]
	prompt = false
[mergetool]
	prompt = false
[color]
	ui = true
